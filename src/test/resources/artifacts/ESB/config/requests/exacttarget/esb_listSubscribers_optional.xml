<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="wso2.connector.exacttarget"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <soapenv:Header></soapenv:Header>
   <soapenv:Body>
      <urn:accessToken>%s(accessToken)</urn:accessToken>
      <urn:apiUrl>%s(apiUrl)</urn:apiUrl>
      <urn:properties>ID,CreatedDate,SubscriberKey,Status</urn:properties>
      <urn:subscriberFilter>
         <Filter>
            <Property>ID</Property>
            <SimpleOperator>equals</SimpleOperator>
            <Value>%s(subscriberId)</Value>
         </Filter>
      </urn:subscriberFilter>
      <urn:method>listSubscribers</urn:method>
   </soapenv:Body>
</soapenv:Envelope>